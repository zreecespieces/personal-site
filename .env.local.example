# Mailchimp API Configuration
# Replace these with your actual Mailchimp API values

# Your Mailchimp API key
MAILCHIMP_API_KEY=your_api_key_here

# Your Mailchimp audience/list ID
MAILCHIMP_AUDIENCE_ID=your_audience_id_here

# The server prefix from your Mailchimp API key (e.g., us1, us2, etc.)
MAILCHIMP_SERVER_PREFIX=your_server_prefix_here

# These are also exposed to the client for the frontend utility
NEXT_PUBLIC_MAILCHIMP_API_KEY=your_api_key_here
NEXT_PUBLIC_MAILCHIMP_AUDIENCE_ID=your_audience_id_here
NEXT_PUBLIC_MAILCHIMP_SERVER_PREFIX=your_server_prefix_here
